type TablaElementoCatalogo {
    id: ID!
    nombre: String
    abreviatura: String
    estado: Boolean
    tablaTiposCatalogo: TablaTiposCatalogo
}

#Root
type Query {
    findAllTablaElementoCatalogos: [TablaElementoCatalogo]!
    countTablaElementoCatalogo: Long!
}

#Root
type Mutation {
    createTablaElementoCatalogo(nombre: String!,
        abreviatura: String!,
        estado: Boolean!,
        tablaTiposCatalogo: TablaTiposCatalogo!): TablaElementoCatalogo!
}

